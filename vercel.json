{
	"version": 2,
	"public": false,
	"alias": "yellowfolder.com",
	"build": {
		"env": {
			"DOMAIN": "yellowfolder.com",
			"NEXT_PUBLIC_FRESHDESK_KEY_PROD": "@next-public-freshdesk-key-prod-production-46mx",
			"NEXT_PUBLIC_FRESHDESK_BASE_URL": "@next-public-freshdesk-base-url-production-2qkq"
		}
	},
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000; includeSubDomains; preload"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; style-src 'self' 'unsafe-inline'; srcipt-src 'self' 'unsafe-inline'; img-src 'self' 'unsafe-inline';"
				},
				{
					"key": "Referrer-Policy",
					"value": "no-referrer"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		}
	]
}
